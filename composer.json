{
    "name": "jpigmans/bui",
    "type": "library",
    "description": "This is the laravel 5.2 package for the lone star warriors buy up indexes. Reason behind this is that we want a fully integrated system with multiple modules. Eventually we will have all packages available for free installing and replicate our upcoming custom platform.",
    "keywords": [
        "jpigmans",
        "bui"
    ],
    "homepage": "https://github.com/crazyjoery",
    "license": "MIT",
    "authors": [
        {
            "name": ":Joery Pigmans",
            "email": ":jpigmans@lone-star-warriors.com",
            "homepage": "#",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "jpigmans\\bui\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jpigmans\\bui\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    }
}
